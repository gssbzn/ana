<%- if controller_name != 'sessions' %>
  <%= link_to t(:sign_in, scope:'session'), new_session_path(resource_name) %><br />
<% end -%>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <%= link_to t(:forgotten_password, scope:'session'), new_password_path(resource_name) %><br />
<% end -%>
